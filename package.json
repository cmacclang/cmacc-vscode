{
  "name": "cmacc-vscode",
  "displayName": "CMACC Preview",
  "description": "A CMACC previewer",
  "version": "2.0.5",
  "publisher": "wilmveel",
  "author": "Willem Veelenturf",
  "license": "MIT",
  "readme": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/wilmveel/cmacc-vscode"
  },
  "galleryBanner": {
    "color": "#78d6f0",
    "theme": "light"
  },
  "bugs": "https://github.com/wilmveel/cmacc-vscode/issues",
  "homepage": "https://github.com/wilmveel/cmacc-vscode/blob/master/README.md",
  "icon": "images/icon.png",
  "main": "./out/src/extension",
  "engines": {
    "vscode": "^1.1.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:cmacc.preview",
    "onCommand:cmacc.previewToSide"
  ],
  "contributes": {
    "keybindings": [
      {
        "command": "cmacc.preview",
        "key": "ctrl+shift+h",
        "when": "editorTextFocus"
      },
      {
        "command": "cmacc.previewToSide",
        "key": "ctrl+k h",
        "when": "editorTextFocus"
      }
    ],
    "commands": [
      {
        "command": "cmacc.preview",
        "title": "Toggle Preview",
        "category": "CMACC"
      },
      {
        "command": "cmacc.previewToSide",
        "title": "Open Preview to the Side",
        "category": "CMACC",
        "icon": {
          "light": "./media/Preview.svg",
          "dark": "./media/Preview_inverse.svg"
        }      }
    ],
    "menus": {
			"editor/title": [
				{
					"command": "cmacc.previewToSide",
					"when": "editorLangId == cmacc",
					"group": "navigation"
				}
			],
			"explorer/context": [
				{
					"command": "cmacc.previewToSide",
					"when": "resourceLangId == cmacc",
					"group": "navigation"
				}
			],
			"commandPalette": [
				{
					"command": "cmacc.previewToSide",
					"when": "editorLangId == cmacc",
					"group": "navigation"
				}
			]
		},
    "languages": [
      {
        "id": "cmacc",
        "aliases": [
          "cmacc",
          "CmAcc"
        ],
        "extensions": [
          ".cmacc"
        ],
        "configuration": "./cmacc.configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "cmacc",
        "scopeName": "scope.cmacc",
        "path": "./syntaxes/cmacc.tmLanguage"
      }
    ]
  },
  "scripts": {
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "dependencies": {
    "cmacc-compiler": "2.0.5",
    "node-fetch": "^1.7.1",
    "node-uuid": "^1.4.8"
  },
  "devDependencies": {
    "typescript": "^1.8.10",
    "vscode": "^0.11.13"
  }
}
